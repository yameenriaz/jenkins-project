pipeline {
  agent any
  stages {
    stage('Build') {
      steps{
        echo 'Build stage'
      }
    }
    stage('Test') {
      steps{
        echo 'Test Stage'
      }
    }
    stage('Staging') {
      steps {
        echo 'This is a staging step'
      }
    }
    stage('Deployment') {
      steps {
        echo 'This is deployment step'
      }
    }
  }
}
